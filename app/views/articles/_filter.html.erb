<%= form_with model: @article, url: '/articles', method: :get do |form| %>
  <%= form.label :country, style: "display: block" %>
  <%= form.select :country, [
    ["-", ""],
    ["de", "de"],
    ["fr", "fr"], 
    ["gb", "gb"], 
    ["it", "it"], 
    ["us", "us"]
  ] %>

  <%= form.label :category, style: "display: block" %>
  <%= form.select :category, [
    ["-", ""],
    ["business", "business"],
    ["entertainment", "entertainment"], 
    ["general", "general"], 
    ["health", "health"], 
    ["science", "science"],
    ["sports", "sports"],
    ["technology", "technology"]
  ] %>
  <%#= form.text_field :apiKey %>
  <%= form.submit "Submit" %>
<% end %>